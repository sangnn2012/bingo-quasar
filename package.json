{
  "name": "bingo-quasar",
  "version": "0.0.2",
  "description": "A Quasar Framework app",
  "productName": "Bingo Quasar",
  "author": "Sang Nguyen <sangnn2012@gmail.com>",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "quasar dev",
    "build": "vue-tsc --noEmit && quasar build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,js,vue,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,vue,css,scss}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "quasar": "^2.14.2",
    "vue": "^3.4.18",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@quasar/app-vite": "^2.0.0-beta.12",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^9.21.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.35",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vite": "^5.1.3",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.1"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
